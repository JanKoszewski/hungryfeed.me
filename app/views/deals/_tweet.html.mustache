<div class="tweet" id=tweet-{{id}}>
  <h2><a href="{{url}}">{{twitter_username}}</a></h2>
  <div class="details">
  <span class="deal_id">{{deal_id}}</span>
  <span class="content">{{content}}</span>
  <span class="link">{{link}}</span>
  <span class="twitter_user_image">{{twitter_user_image}}</span>
  <span class="klout_score">{{klout_score}}</span>
  </div>
</div>